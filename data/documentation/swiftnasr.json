":"topic","navigatorTitle":[{"kind":"identifier","text":"Navaid"}],"url":"\/documentation\/swiftnasr\/navaid"},"doc://SwiftNASR/documentation/SwiftNASR":{"role":"collection","title":"SwiftNASR","abstract":[{"type":"text","text":"A library for downloading and working with aeronautical data from the FAA’s"},{"type":"text","text":" "},{"type":"text","text":"Facility Aeronautical Data Distribution System (FADDS)."}],"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR","kind":"symbol","type":"topic","url":"\/documentation\/swiftnasr"},"doc://SwiftNASR/documentation/SwiftNASR/FSS":{"role":"symbol","title":"FSS","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"FSS"}],"abstract":[{"type":"text","text":"Flight service stations are FAA facilities that provide in-flight information"},{"type":"text","text":" "},{"type":"text","text":"to pilots but do not provide separation services (except in limited local cases"},{"type":"text","text":" "},{"type":"text","text":"when the FSS is associated with an uncontrolled airport)."}],"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/FSS","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"FSS"}],"url":"\/documentation\/swiftnasr\/fss"},"doc://SwiftNASR/documentation/SwiftNASR/JSONZipDecoder":{"role":"symbol","title":"JSONZipDecoder","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"JSONZipDecoder"}],"abstract":[],"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/JSONZipDecoder","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"JSONZipDecoder"}],"url":"\/documentation\/swiftnasr\/jsonzipdecoder"},"doc://SwiftNASR/documentation/SwiftNASR/NASRData":{"role":"symbol","title":"NASRData","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"NASRData"}],"abstract":[{"type":"text","text":"A Codable class containing all data parsed from a NASR distribution. The"},{"type":"text","text":" "},{"type":"text","text":"members of this class will be "},{"type":"codeVoice","code":"nil"},{"type":"text","text":" until their respective calls to"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/NASR\/parse(_:withProgress:errorHandler:completionHandler:)"},{"type":"text","text":" have been made."}],"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/NASRData","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"NASRData"}],"url":"\/documentation\/swiftnasr\/nasrdata"},"doc://SwiftNASR/documentation/SwiftNASR/ArchiveDataDistribution":{"role":"symbol","title":"ArchiveDataDistribution","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"ArchiveDataDistribution"}],"abstract":[{"type":"text","text":"A NASR distribution that has been loaded from a ZIP archive and stored in"},{"type":"text","text":" "},{"type":"text","text":"memory. ZIPFoundation is used to extract the archive, which is loaded in"},{"type":"text","text":" "},{"type":"text","text":"buffered chunks."}],"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/ArchiveDataDistribution","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"ArchiveDataDistribution"}],"url":"\/documentation\/swiftnasr\/archivedatadistribution"},"doc://SwiftNASR/documentation/SwiftNASR/ARTCC":{"role":"symbol","title":"ARTCC","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"ARTCC"}],"abstract":[{"type":"text","text":"An air route traffic control center (“Center”) is a facility that provides"},{"type":"text","text":" "},{"type":"text","text":"separate services to enroute air traffic. Each center facility controls traffic"},{"type":"text","text":" "},{"type":"text","text":"within its flight information region (FIR) or upper information region (UIR)."},{"type":"text","text":" "},{"type":"text","text":"Centers are split by the FIR they control, their location name, and the site"},{"type":"text","text":" "},{"type":"text","text":"type. There is one entry for each combination of those three fields."}],"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/ARTCC","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"ARTCC"}],"url":"\/documentation\/swiftnasr\/artcc"},"doc://SwiftNASR/documentation/SwiftNASR/GettingStarted":{"role":"article","title":"Getting Started","abstract":[{"type":"text","text":"A basic overview of how to use SwiftNASR to load and parse FAA aeronautical"},{"type":"text","text":" "},{"type":"text","text":"data."}],"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/GettingStarted","kind":"article","type":"topic","url":"\/documentation\/swiftnasr\/gettingstarted"},"doc://SwiftNASR/documentation/SwiftNASR/NASR":{"role":"symbol","title":"NASR","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"NASR"}],"abstract":[{"type":"text","text":"The "},{"type":"codeVoice","code":"NASR"},{"type":"text","text":" class provides top-level access to loading, parsing, and accessing"},{"type":"text","text":" "},{"type":"text","text":"aeronautical data disseminated via NASR distributions."}],"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/NASR","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"NASR"}],"url":"\/documentation\/swiftnasr\/nasr"},"doc://SwiftNASR/documentation/SwiftNASR/Error":{"role":"symbol","title":"Error","fragments":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"Error"}],"abstract":[{"type":"text","text":"Errors that can occur in SwiftNASR methods."}],"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/Error","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"Error"}],"url":"\/documentation\/swiftnasr\/error"},"doc://SwiftNASR/documentation/SwiftNASR/Records":{"role":"collectionGroup","title":"Records","abstract":[{"type":"text","text":"SwiftNASR records represent the data types present in a NASR distribution."},{"type":"text","text":" "},{"type":"text","text":"After one of the "},{"type":"codeVoice","code":"load"},{"type":"text","text":" methods is called (see "},{"type":"emphasis","inlineContent":[{"type":"text","text":"Loaders"}]},{"type":"text","text":"), records can be"},{"type":"text","text":" "},{"type":"text","text":"parsed."}],"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/Records","kind":"article","type":"topic","url":"\/documentation\/swiftnasr\/records"},"doc://SwiftNASR/documentation/SwiftNASR/Loader":{"role":"symbol","title":"Loader","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"Loader"}],"abstract":[{"type":"text","text":"Loaders create a "},{"type":"reference","isActive":true,"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/Distribution"},{"type":"text","text":" from a NASR archive, on disk, in memory, or"},{"type":"text","text":" "},{"type":"text","text":"downloaded from the Internet (see "},{"type":"reference","isActive":true,"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/Downloader"},{"type":"text","text":"). For example,"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/ArchiveDataDownloader"},{"type":"text","text":" produces a ZIP-compressed archive, and so the"},{"type":"text","text":" "},{"type":"text","text":"data would need to be handled by "},{"type":"reference","isActive":true,"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/ArchiveDataDistribution"},{"type":"text","text":". The"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/ArchiveLoader"},{"type":"text","text":" implementation mediates between the two classes."}],"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/Loader","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"Loader"}],"url":"\/documentation\/swiftnasr\/loader"},"doc://SwiftNASR/documentation/SwiftNASR/Distribution":{"role":"symbol","title":"Distribution","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"Distribution"}],"abstract":[{"type":"text","text":"Protocol that describes classes that can load NASR data from different ways of"},{"type":"text","text":" "},{"type":"text","text":"storing distribution data."}],"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/Distribution","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"Distribution"}],"url":"\/documentation\/swiftnasr\/distribution"},"doc://SwiftNASR/documentation/SwiftNASR/ArchiveDataDownloader":{"role":"symbol","title":"ArchiveDataDownloader","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"ArchiveDataDownloader"}],"abstract":[{"type":"text","text":"A downloader that downloads a distribution archive into memory. No data is"},{"type":"text","text":" "},{"type":"text","text":"saved to disk and no buffering is done."}],"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/ArchiveDataDownloader","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"ArchiveDataDownloader"}],"url":"\/documentation\/swiftnasr\/archivedatadownloader"},"doc://SwiftNASR/documentation/SwiftNASR/Airport":{"role":"symbol","title":"Airport","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"Airport"}],"abstract":[{"type":"text","text":"A landing facility, including airports, seaports, heliports, balloonports, etc."}],"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/Airport","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"Airport"}],"url":"\/documentation\/swiftnasr\/airport"},"doc://SwiftNASR/documentation/SwiftNASR/Cycle":{"role":"symbol","title":"Cycle","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"Cycle"}],"abstract":[{"type":"text","text":"NASR data is distributed on a 56-day cycle. The Cycle class represents one such"},{"type":"text","text":" "},{"type":"text","text":"time period. Cycles are defined by the first day of their effectivity period."}],"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/Cycle","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"Cycle"}],"url":"\/documentation\/swiftnasr\/cycle"},"doc://SwiftNASR/documentation/SwiftNASR/Loaders":{"role":"collectionGroup","title":"Working with Loaders","abstract":[{"type":"text","text":"Subclasses of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/Loader"},{"type":"text","text":" load NASR distributions, either from disk or from the"},{"type":"text","text":" "},{"type":"text","text":"FADDS website. Loaders are responsible for loading the raw data and wrapping it"},{"type":"text","text":" "},{"type":"text","text":"in a corresponding "},{"type":"reference","isActive":true,"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/Distribution"},{"type":"text","text":" instance, which will handle parsing the"},{"type":"text","text":" "},{"type":"text","text":"data."}],"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/Loaders","kind":"article","type":"topic","url":"\/documentation\/swiftnasr\/loaders"},"doc://SwiftNASR/documentation/SwiftNASR/Distributions":{"role":"collectionGroup","title":"Working with Distributions","abstract":[{"type":"text","text":"Distributions are the files that contain the NASR data, as downloaded from the"},{"type":"text","text":" "},{"type":"text","text":"FADDS website. You can use subclasses of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/Distribution"},{"type":"text","text":" to automatically"},{"type":"text","text":" "},{"type":"text","text":"download those files, or work with previously-downloaded files."}],"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/Distributions","kind":"article","type":"topic","url":"\/documentation\/swiftnasr\/distributions"},"doc://SwiftNASR/documentation/SwiftNASR/Downloader":{"role":"symbol","title":"Downloader","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"Downloader"}],"abstract":[{"type":"text","text":"A downloader is a class that can download a NASR distribution from the FAA’s"},{"type":"text","text":" "},{"type":"text","text":"website. This abstract superclass contains functionality common to all"},{"type":"text","text":" "},{"type":"text","text":"downloaders."}],"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/Downloader","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"Downloader"}],"url":"\/documentation\/swiftnasr\/downloader"},"doc://SwiftNASR/documentation/SwiftNASR/NASR/parse(_:withProgress:errorHandler:completionHandler:)":{"role":"symbol","title":"parse(_:withProgress:errorHandler:completionHandler:)","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"parse"},{"kind":"text","text":"("},{"kind":"typeIdentifier","text":"RecordType","preciseIdentifier":"s:9SwiftNASR10RecordTypeO"},{"kind":"text","text":", "},{"kind":"externalParam","text":"withProgress"},{"kind":"text","text":": ("},{"kind":"typeIdentifier","text":"Progress","preciseIdentifier":"c:objc(cs)NSProgress"},{"kind":"text","text":") -> "},{"kind":"typeIdentifier","text":"Void","preciseIdentifier":"s:s4Voida"},{"kind":"text","text":", "},{"kind":"externalParam","text":"errorHandler"},{"kind":"text","text":": ("},{"kind":"typeIdentifier","text":"Error","preciseIdentifier":"s:s5ErrorP"},{"kind":"text","text":") -> "},{"kind":"typeIdentifier","text":"Bool","preciseIdentifier":"s:Sb"},{"kind":"text","text":", "},{"kind":"externalParam","text":"completionHandler"},{"kind":"text","text":": () -> "},{"kind":"typeIdentifier","text":"Void","preciseIdentifier":"s:s4Voida"},{"kind":"text","text":") "},{"kind":"keyword","text":"throws"}],"abstract":[{"type":"text","text":"Parses data of a certain type (e.g., airports) from the NASR distribution."},{"type":"text","text":" "},{"type":"text","text":"Populates the corresponding field in the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/NASRData"},{"type":"text","text":" field of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/NASR\/data"},{"type":"text","text":"."}],"identifier":"doc:\/\/SwiftNASR\/documentation\/SwiftNASR\/NASR\/parse(_:withProgress:errorHandler:completionHandler:)","kind":"symbol","type":"topic","url":"\/documentation\/swiftnasr\/nasr\/parse(_:withprogress:errorhandler:completionhandler:)"}}}